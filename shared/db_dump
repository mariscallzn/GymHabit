INSERT INTO exercise_types VALUES(6,'Strongman');
CREATE TABLE IF NOT EXISTS `exercises` (`uid` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, `name` TEXT NOT NULL, `description` TEXT NOT NULL, `muscle_id` INTEGER NOT NULL, `equipment_id` INTEGER NOT NULL, `exercise_type_id` INTEGER NOT NULL, FOREIGN KEY(`muscle_id`) REFERENCES `muscles`(`uid`) ON UPDATE NO ACTION ON DELETE NO ACTION , FOREIGN KEY(`equipment_id`) REFERENCES `equipments`(`uid`) ON UPDATE NO ACTION ON DELETE NO ACTION , FOREIGN KEY(`exercise_type_id`) REFERENCES `exercise_types`(`uid`) ON UPDATE NO ACTION ON DELETE NO ACTION );
