CREATE INDEX `index_tracking_session_id` ON `tracking` (`session_id`);
CREATE INDEX `index_tracking_set_type_id` ON `tracking` (`set_type_id`);
CREATE INDEX `index_session_comments_session_id` ON `session_comments` (`session_id`);
CREATE INDEX `index_sets_tracking_id` ON `sets` (`tracking_id`);
CREATE INDEX `index_sets_exercise_id` ON `sets` (`exercise_id`);
CREATE INDEX `index_exercises_muscle_id` ON `exercises` (`muscle_id`);
CREATE INDEX `index_exercises_equipment_id` ON `exercises` (`equipment_id`);
CREATE INDEX `index_exercises_exercise_type_id` ON `exercises` (`exercise_type_id`);
COMMIT;