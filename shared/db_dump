-- I'll keep this file in order to give it a try latter about pre-populate the database
-- using assets
PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS `tracking` (`uid` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, `session_id` INTEGER NOT NULL, `set_type_id` INTEGER NOT NULL, FOREIGN KEY(`session_id`) REFERENCES `sessions`(`uid`) ON UPDATE NO ACTION ON DELETE NO ACTION , FOREIGN KEY(`set_type_id`) REFERENCES `set_types`(`uid`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `sessions` (`uid` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, `title` TEXT NOT NULL, `created_time` TEXT NOT NULL, `planned_time` TEXT NOT NULL, `completed_time` TEXT NOT NULL);
CREATE TABLE IF NOT EXISTS `session_comments` (`uid` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, `session_id` INTEGER NOT NULL, `comment` TEXT NOT NULL, FOREIGN KEY(`session_id`) REFERENCES `sessions`(`uid`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `sets` (`uid` INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, `order` TEXT NOT NULL, `rep_speed_goal` INTEGER NOT NULL, `rep_speed_real` INTEGER NOT NULL, `reps_goal` INTEGER NOT NULL, `reps_real` INTEGER NOT NULL, `amount_goal` INTEGER NOT NULL, `amount_real` INTEGER NOT NULL, `rest_time_goal` INTEGER NOT NULL, `rest_time_real` INTEGER NOT NULL, `started_time` TEXT NOT NULL, `completed_time` TEXT NOT NULL, `tracking_id` INTEGER NOT NULL, `exercise_id` INTEGER NOT NULL, FOREIGN KEY(`tracking_id`) REFERENCES `tracking`(`uid`) ON UPDATE NO ACTION ON DELETE NO ACTION , FOREIGN KEY(`exercise_id`) REFERENCES `exercises`(`uid`) ON UPDATE NO ACTION ON DELETE NO ACTION );
CREATE TABLE IF NOT EXISTS `set_types` (`uid` INTEGER NOT NULL, `name` TEXT NOT NULL, PRIMARY KEY(`uid`));
CREATE TABLE IF NOT EXISTS `muscles` (`uid` INTEGER NOT NULL, `name` TEXT NOT NULL, PRIMARY KEY(`uid`));
